{
  "name": "@destiny-definitions/common",
  "version": "0.0.1",
  "license": "AGPL",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "rm -fr dist/* && tsc"
  },
  "main": "./dist/index.js",
